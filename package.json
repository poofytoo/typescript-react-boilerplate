{
  "name": "project-name",
  "description": "Project Description",
  "private": true,
  "author": "Project Author",
  "license": "Unlicensed",
  "scripts": {
    "build": "run-p build:*",
    "build:app": "webpack --config webpack.config.js",
    "build:server": "tsc -p server/",
    "lint": "run-s lint:*",
    "lint:app": "tslint --project app/src --config tslint.json",
    "lint:server": "tslint --project server --config tslint.json",
    "lint-fix": "run-s lint:fix:*",
    "lint:fix:app": "tslint --project app/src  --config tslint.json --fix",
    "lint:fix:server": "tslint --project server  --config tslint.json --fix",
    "start": "nodemon ./server/server.js",
    "start:prod": "NODE_ENV=production npm run start",
    "dev": "npm-run-all -s build -p watch start",
    "dev:offline": "NODE_ENV=offline yarn dev",
    "dist": "NODE_ENV=production npm run build",
    "watch": "run-p watch:*",
    "watch:app": "webpack --watch --profile --colors --progress --config webpack.config.js",
    "watch:server": "tsc -w -p server/"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/cookie-parser": "^1.3.30",
    "@types/cookie-session": "^2.0.34",
    "@types/express": "^4.0.36",
    "@types/node": "^14.0.14",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "css-loader": "^3.6.0",
    "file-loader": "^6.0.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.0.2",
    "prettier": "^2.0.5",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^1.0.1",
    "style-loader": "^1.2.1",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^5.0.0",
    "typescript": "^3.9.5",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "awesome-typescript-loader": "^5.2.1",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.17.3",
    "hbs": "^4.0.1",
    "mini-css-extract-plugin": "^0.9.0",
